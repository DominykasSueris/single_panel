{
  "name": "watchland",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-sdk/types": "^3.159.0",
    "@reduxjs/toolkit": "^1.8.5",
    "@types/react-datepicker": "^4.8.0",
    "aws-sdk": "^2.1195.0",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "uuid": "^9.0.0",
    "watch-land-ts-client": "^1.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "react-scripts start && node-sass src/ -0 src/ -w -r",
    "generate": "npx generate-react-cli component",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently npm:backend:*",
    "backend:aws": "json-server --watch ./data/aws.json --port 3004",
    "backend:google": "json-server --watch ./data/google.json --port 3005",
    "backend:azure": "json-server --watch ./data/azure.json --port 3006"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.41",
    "@types/node-sass": "^4.11.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.5",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.3.0",
    "generate-react-cli": "^7.2.0",
    "i": "^0.3.7",
    "json-server": "^0.17.0",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4"
  }
}
